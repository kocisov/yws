{
  "name": "yws",
  "description": "WebSocket Server/Client Wrapper",
  "version": "0.0.2",
  "author": "kocisov",
  "bugs": "https://github.com/kocisov/yws/issues",
  "dependencies": {
    "isomorphic-ws": "^4.0.1",
    "nanoid": "^3.1.30"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "@types/ws": "^8.2.2",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "tsup": "^5.11.9",
    "typescript": "^4.5.4"
  },
  "exports": {
    "./server": "./dist/server.js",
    ".": "./dist/client.js"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/kocisov/yws",
  "keywords": [
    "Client",
    "Server",
    "WebSocket",
    "Wrapper"
  ],
  "license": "MIT",
  "main": "dist/client.js",
  "module": "dist/client.mjs",
  "repository": "https://github.com/kocisov/yws.git",
  "scripts": {
    "build": "NODE_ENV=production tsup src/server.ts src/client.ts --dts --format esm,cjs --env.NODE_ENV production --minify",
    "prepare": "yarn build",
    "test": "jest"
  },
  "types": "dist/client.d.ts",
  "typesVersions": {
    "*": {
      "server": [
        "dist/server.d.ts"
      ]
    }
  }
}